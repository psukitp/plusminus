{
  "name": "plusminus_frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cruise": "npx depcruise --include-only \"^src\" --output-type dot --config .dependency-cruiser.cjs ./ | dot -Gsplines=ortho -Grankdir=LR -Tsvg | depcruise-wrap-stream-in-html > dependencygraph.html"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.63.0",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "echarts": "^5.5.1",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-toastify": "^11.0.2",
    "styled-components": "^6.1.11",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/echarts": "^4.9.22",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "dependency-cruiser": "^16.4.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "less": "^4.2.0",
    "prettier": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-svgr": "^4.2.0"
  }
}
